{
    "doctype": "DocType",
    "name": "service_object",
    "module": "Ferum Customs",
    "engine": "InnoDB",
    "track_changes": 1,
    "title_field": "object_name",
    "fields": [
        {
            "fieldname": "object_name",
            "label": "Название объекта",
            "fieldtype": "Data",
            "reqd": 1,
            "in_list_view": 1,
            "unique": 1
        },
        {
            "fieldname": "customer",
            "label": "Клиент",
            "fieldtype": "Link",
            "options": "Customer",
            "in_list_view": 1
        },
        {
            "fieldname": "location",
            "label": "Расположение",
            "fieldtype": "Data"
        },
        {
            "fieldname": "description",
            "label": "Описание",
            "fieldtype": "Text"
        },
        {
            "fieldname": "status",
            "label": "Статус",
            "fieldtype": "Select",
            "options": "Active\nInactive\nUnder Maintenance",
            "default": "Active",
            "in_list_view": 1
        }
    ],
    "permissions": [
        {
            "role": "Проектный менеджер",
            "read": 1,
            "write": 1,
            "create": 1,
            "delete": 1
        },
        {
            "role": "Инженер",
            "read": 1
        }
    ]
}
